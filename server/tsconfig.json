{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES6"
    ],
    "paths": {
      "@/*": ["./src/*"]
    },
    "noEmit": true,
    "module": "CommonJS",
    "rootDir": "src",
    "resolveJsonModule": true,
    "allowJs": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true
  }
}